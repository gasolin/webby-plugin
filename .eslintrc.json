{
  "parser": "babel-eslint",
  "extends": "eslint-config-defaults/configurations/google",
  "rules": {
    "dot-location": 0,
    "newline-after-var": 0,
    "new-cap": 0,
    "no-invalid-this": 0,
    "valid-jsdoc": 0
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  }
}
